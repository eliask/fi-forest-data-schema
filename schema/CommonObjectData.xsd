		<xs:annotation>
			<xs:documentation xml:lang="fi"></xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Tieto1" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Tieto2" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Tieto3" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Tieto4" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Tieto5" type="xs:string" minOccurs="1" maxOccurs="1"/>
