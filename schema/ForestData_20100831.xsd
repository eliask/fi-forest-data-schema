<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://standardit.tapio.fi/schemas/forestData/2010/08/31"
	xmlns="http://standardit.tapio.fi/schemas/forestData/2010/08/31"
	xmlns:re="http://standardit.tapio.fi/schemas/forestData/realEstate/2010/08/31"
	elementFormDefault="qualified"
	attributeFormDefault="qualified"
	version="1.0">

	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/realEstate/2010/08/31"
				schemaLocation="RealEstate_20100831.xsd"/>

	<!-- Elementtimääritykset -->
	<!-- Element definitions -->
	<xs:element name="ForestPropertyData" type="ForestPropertyDataType"/>

	<!-- Tyyppimääritykset -->
	<!-- Type definitions -->
	<xs:complexType name="ForestPropertyDataType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Metsävaratiedot kiinteistöittäin.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en">Forest property data grouped by real estates.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="re:RealEstates"/>
		</xs:sequence>
	</xs:complexType>

</xs:schema>
