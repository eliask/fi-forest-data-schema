			<xs:documentation xml:lang="en">ForestUseDeclaration.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="fud:ForestUseDeclaration" minOccurs="1" maxOccurs="unbounded"/>
