	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/ForestUseDeclaration"  schemaLocation="../V6/ForestUseDeclaration.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/common" schemaLocation="../V6/Common.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/contactInformation" schemaLocation="../V6/ContactInformation.xsd"/>
