<!--
	***************************************************************
	Metsäkeskussanomien yleiset elementit ja tyypit.
	Skeema on JHS-170 -suosituksen mukainen.
	Ensimmäinen versio 1.0
	18.9.2015
	
	Muutokset skeemapakettiin V10.05:
	-Skeematiedoston xs:schema elementtiin muutettu versionumeroksi "V10.05". Jatkossa tämä versionumero tulee päivittymään tarpeen mukaan silloin kun tiedostoon tehdään muutoksia.
	
	Muutokset skeemapakettiin V11.01:
	-Siirretty tyyppimäärittely AttorneyType tiedostosta FinancingActCommon.xsd tiedostoon ForestCentreCommon.xsd. Siirrytty käyttämään elementtejä PowerOfAttorney ja RightToSpecifyBankAccountsOfPaymentTransactions viittauksella (ref). Viittaus skeematiedostoon FinancingActCommon.xsd.
	
	Muutokset skeemapakettiin V11.02:
	-Skeematiedoston FinancingActCommon.xsd sisältö siirretty tähän skeematiedostoon.
	
	Muutokset skeemapakettiin V11.05:
	-Lisätty uusi elementtimäärittely Document, tyypiltään co:Document4MBType. Viittaus complexType DocumentsType:ssä muutettu käyttämään tätä uutta elementtimäärittelyä. Lisäksi elementin maxOccurs muutettu "n" -> 10
	***************************************************************
-->

<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://standardit.tapio.fi/schemas/forestData/ForestCentreCommon"
	xmlns="http://standardit.tapio.fi/schemas/forestData/ForestCentreCommon"
	xmlns:co="http://standardit.tapio.fi/schemas/forestData/common"
	xmlns:ci="http://standardit.tapio.fi/schemas/forestData/contactInformation"
	xmlns:stb="http://standardit.tapio.fi/schemas/forestData/standbase"
	xmlns:re="http://standardit.tapio.fi/schemas/forestData/realEstate"
	xmlns:gdt="http://standardit.tapio.fi/schemas/forestData/common/geometricDataTypes"
	xmlns:xmime="http://www.w3.org/2005/05/xmlmime"
	xmlns:gml="http://www.opengis.net/gml"
	elementFormDefault="qualified"
	version="V11.05">

	<!-- Imports / Includes -->
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/common"
		schemaLocation="Common.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/contactInformation"
		schemaLocation="ContactInformation.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/standbase"
		schemaLocation="StandBase.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/realEstate"
		schemaLocation="RealEstateBase.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/common/geometricDataTypes"
		schemaLocation="Common_GeometricDataType.xsd"/>
	<xs:import namespace="http://www.w3.org/2005/05/xmlmime"
		schemaLocation="../w3c/2005/xmlmime.xsd"/>
	<xs:import namespace="http://www.opengis.net/gml" schemaLocation="../gml/3.1.1/base/gml.xsd"/>

    <!-- Elementtimääritykset -->
    <!-- Element definitions -->
    <xs:element name="SubsidyApplierId" type="co:IdStringNotEmptyType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Id viittaa id-attribuuttiin henkilölistassa. Kiinteistön omistaja on siis tässä viitattu henkilö / toimija.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="RealEstateId" type="co:IdStringNotEmptyType"/>
    <xs:element name="PayeeId" type="co:IdStringNotEmptyType"/>
    <xs:element name="SubsidyApplierReference" type="SubsidyApplierReferenceType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Tuensaajat (kiintestön omistajat) tai muut maksunsaajat</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FinancingActApplicationReference" type="co:ReferenceType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Rahoitushakemuksen yksilöivä viite. Hakijan ilmoittama hakemuskohtainen, valtakunnallisesti yksilöivä viite. Hakemusviitteen muodostamissääntö: lähettäjän organisaatiotunnus, vuosi, lähettäjän antama ilmoituksen yksilöivä tunnus. Lähettävän organisaation tunnukset: Metsähallitus=M-H, Metsämannut=M-M, Metsänhoitoyhdistys=MHY, Stora Enso=S-E, Tornator=TOR, UPM-Kymmene=UPM, MetsäGroup=M-G, Metsään.fi = MFI, L&amp;T Biowatti Oy = BWO, Muu organisaatio=MUU. Esim. MHY2015999999999999999999999. Jos siirtotiedostossa käytetään MUU-organisaatiotunnusta, on siitä oltava yhteydessä tiedonsiirtopalvelun ylläpitäjään.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Reference of FinancingActApplication. Applier announces.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="CompletionDeclarationReference" type="co:ReferenceType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Toteutusilmoituksen yksilöivä viite. Ilmoituksen lähettäjän antama ilmoituskohtainen, valtakunnallisesti yksilöivä viite. Ilmoitusviitteen muodostamissääntö: lähettäjän organisaatiotunnus, vuosi, lähettäjän antama ilmoituksen yksilöivä tunnus.  Lähettävän organisaation tunnukset:  Metsähallitus=M-H, Metsämannut=M-M, Metsänhoitoyhdistys=MHY, Stora Enso=S-E, Tornator=TOR, UPM-Kymmene=UPM, MetsäGroup=M-G, Metsään.fi = MFI,  L&amp;T Biowatti Oy = BWO, Muu organisaatio=MUU. Esim. MHY2015999999999999999999999. Jos siirtotiedostossa käytetään MUU-organisaatiotunnusta, on siitä oltava yhteydessä tiedonsiirtopalvelun ylläpitäjään.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Reference of Completion Declaration</xs:documentation>
        </xs:annotation>
    </xs:element>
	<xs:element name="CustomerReference" type="co:ReferenceType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Asiakasviite</xs:documentation>
			<xs:documentation xml:lang="sv"/>
			<xs:documentation xml:lang="en"/>
		</xs:annotation>
	</xs:element>
	<xs:element name="StandReference" type="co:ReferenceType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Hakemuksen lähettäjän antama kuviokohtainen, valtakunnallisesti yksilöivä viite. Kuvioviitteen muodostamissääntö: lähettäjän organisaatiotunnus, vuosi, lähettäjän antama kuvion yksilöivä tunnus. Lähettävän organisaation tunnukset: Metsähallitus=M-H, Metsämannut=M-M, Metsänhoitoyhdistys=MHY, Stora Enso=S-E, Tornator=TOR, UPM-Kymmene=UPM, MetsäGroup=M-G, Metsään.fi = MFI, L&amp;T Biowatti Oy = BWO, Muu organisaatio=MUU. Esim. MHY2003999999999999999999999. Jos siirtotiedostossa käytetään MUU-organisaatiotunnusta, on siitä oltava yhteydessä tiedonsiirtopalvelun ylläpitäjään.</xs:documentation>
			<xs:documentation xml:lang="sv"/>
			<xs:documentation xml:lang="en"/>
		</xs:annotation>
	</xs:element>
	<xs:element name="FinancingActApplicationTextInformation" type="co:String2000Type">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hakemuksen lisätiedot</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FinancingActCompletionDeclarationTextInformation" type="co:String2000Type">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Toteutusilmoituksen lisätiedot</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="UpdatePreviousMessage" type="co:YesNoType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kyllä- / Ei-tieto, jolla ilmoituksen lähettäjä ilmaisee korvaako hän aikaisemman hakemuksen lähettämällä uuden täydellisen hakemuksen vai lähettääkö hän uutta hakemusta. Arvo 0 uudella hakemuksella ja arvo 1 päivittävällä hakemuksella.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FinancingActNumber" type="FinancingActNumberType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hankkeen Riihinumero. Perusrahoituspäätöksessä mainittu numero (hankenumero), esim. 11-2016-123456.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="RemovalClass" type="co:RemovalClassType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Poistumaluokka</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="HeightClass" type="co:HeightClassType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Pituusluokka</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="DiameterClass" type="co:DiameterClassType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Läpimittaluokka (pohjapinta-alalla painotettu keskiläpimitta)</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="SmallWoodRemovalClass" type="co:SmallWoodRemovalClassType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Pienpuun poistumaluokka</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="ElectronicNotification" type="co:ElectronicNotificationType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Suostumus sähköiseen tiedonantoon.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="PaymentsDate" type="co:DateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Maksupäivä</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Arrival date</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="ArrivalDate" type="co:DateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Saapumispäivä</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Arrival date</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="CostType" type="CostTypeType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kustannuslajin tiedot</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="CostTypeNumber" type="co:CostTypeNumberType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kustannuslajin numero</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FertilizationType" type="co:String50Type">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Lannoitustyyppi</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="CompletedWorkAmount" type="AmountType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Toteutetun työn määrä. Pinta-alan tapauksessa kyse on metsäpinta-alasta. Mikäli paikkatieto eli geometria sisältää esim. metsätien, tulee toteutetun pinta-alan käsittää vain metsäpinta-ala.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="PlannedWorkAmount" type="AmountType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Suunniteltu työmäärä. Pinta-alan tapauksessa kyse on metsäpinta-alasta. Mikäli paikkatieto eli geometria sisältää esim. metsätien, tulee toteutetun pinta-alan käsittää vain metsäpinta-ala.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="UnitPrice" type="UnitPriceType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Yksikköhinta</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="EstimatedStartDate" type="co:DateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hankkeen arvioitu alkamisaika</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="EstimatedEndDate" type="co:DateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hankkeen arvioitu päättymisaika</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="StartDate" type="co:DateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hankkeen alkamisaika</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="EndDate" type="co:DateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hankkeen päättymisaika</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="SubsidyAmount" type="co:MoneyType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Tuen määrä</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FinancingActWorkGroup" type="co:FinancingActWorkGroupType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Työlajiryhmä jota hakemus koskee. Eritelty tukilaji määritellään PartsOfProject elementin alla.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FinancingActWorkCode" type="co:FinancingActWorkCodeType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Työlaji jota hakemus koskee</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="AreaNo" type="co:PositiveInteger4digitsType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hankeosan numero. Yksittäisen hakemuksen / toteutusilmoituksen hankeosan yksilöivä numero.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="StandNumber" type="stb:StandNumberType"/>
    <xs:element name="StandNumberExtension" type="stb:StandNumberExtensionType"/>
    <xs:element name="Area" type="stb:AreaType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kuvion paikkatiedon / geometrian pinta-ala hehtaareina. Mikäli kuviointi sisältää myös teitä ja linjoja (mm. sähkölinja) eli kuviorajojen sisällä on kyseisiä kohteita, pinta-ala sisältää myös näiden pinta-alan.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="MainGroup" type="co:ExtendedMainGroupType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kuvion pääryhmä.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="SubGroup" type="co:SubGroupType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kuvion alaryhmä.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FertilityClass" type="co:FertilityClassType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kuvion kasvupaikkaluokka.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="SoilType" type="co:SoilTypeType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kuvion maalaji.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="MainTreeSpecies" type="co:TreeSpeciesConciseType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Pääpuulaji</xs:documentation>
            <xs:documentation xml:lang="en">Main tree species</xs:documentation>
            <xs:documentation xml:lang="sv"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="CuttingStemCount" type="CuttingStemCountType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Poistuman runkoluku</xs:documentation>
            <xs:documentation xml:lang="en"/>
            <xs:documentation xml:lang="sv"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="StemCount" type="co:StemCountType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Puuston runkoluku, kpl/ha</xs:documentation>
            <xs:documentation xml:lang="en"/>
            <xs:documentation xml:lang="sv"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="MeanDiameter" type="co:DiameterType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Keskiläpimitta, cm (pohjapinta-alalla painotettu keskiläpimitta)</xs:documentation>
            <xs:documentation xml:lang="en"/>
            <xs:documentation xml:lang="sv"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="MeanHeight" type="co:HeightType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Keskipituus, m</xs:documentation>
            <xs:documentation xml:lang="en"/>
            <xs:documentation xml:lang="sv"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="SmallWoodRemovalVolume" type="co:PositiveInteger4digitsType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Pienpuun poistuman tilavuus m3/ha.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="LocationEstate" type="LocationEstateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Id-viittaus RealEstate.id -attribuuttiin.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="StandExtraInfo" type="co:String2000Type"/>
    <xs:element name="Date" type="co:DateType"/>
    <xs:element name="Sender" type="ci:ContactInformationType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hakemuksen lähettäjä / laatija. Sähköpostiosoite (EmailAddress) on yhteydenotto-osoite sähköisessä yhteydenpidossa hakemuksen / toteutusilmoituksen lähettäjään / laatijaan. Käyttötarkoitukset esim. yhteydenotot hakemuksen / toteutusilmoituksen lähettäjään ja päätöksen toimittaminen.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="Attorney" type="AttorneyType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Asiamies ja yhteyshenkilö</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Attorney and contact person</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="PowerOfAttorney" type="PowerOfAttorneyType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Asiamiehellä on valtakirja oikeudesta edustaa hakemusasiassa.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="RightToSpecifyBankAccountsOfPaymentTransactions" type="co:YesNoType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Asiamiehellä on oikeus ohjata pankkiliikenne toiselle tilille.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="WorkingRepresentatives" type="WorkingRepresentativesType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Rahoitushakemuksen mukaisten töiden toteutuksesta vastuussa olevat toimijat.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="WorkingRepresentative" type="WorkingRepresentativeType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Rahoitushakemuksen mukaisten töiden toteutuksesta vastuussa olevan toimijan tiedot mm. tiedotusta varten.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="SentDate" type="co:DateType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Sanoman lähetyspvm. Myös päivitettävällä sanomalla sanoman uuden sanoman lähetyspvm.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="CopOperationProject" type="co:YesNoType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hanke toteutetaan eri tuensaajien yhteishankkeena</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="Language" type="co:ISO639char2LanguageType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Hakemuksen kieli, määrää asiointikielen.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="PaymentsReference" type="PaymentsReferenceType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Maksuviite</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="ProjectIsAuthorizedForEstate" type="co:YesNoType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kiinteistön mailla on lupa toteuttaa hanke. Kiinteistö ei osallistu itse hankkeen toteuttamiseen.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
    </xs:element>
    <xs:element name="FinancingActRealEstates" type="FinancingActRealEstatesType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Rahoitushakemuksen tuensaaja- ja lupakiinteistöt.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
